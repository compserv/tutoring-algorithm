// each value is described by "<JSON TYPE>:<DESCRIPTION>"

{
    // array of strings which denote to classes
    "courseNames":"array:strings, indices correspond to 'courses' arrays",

    // array of tutor objects
    "tutors":
    [
        {
        "tid":"int:tutor uid",
        "name":"string:tutor name",
        // 0=disprefer (often cannot make), 1=ambivalent, 2=prefer
        "slots":"array:ints in [0,2] describing preference level of each slot",
        // 0=have not taken, 1=taken, 2=prefer
        "courses":"array:ints in [0,2] describing preference level per course",
        // 0=disprefer, 1=ambivalent, 2=prefer
        "adjacentPref":"int in [0,2] describing preference level"
        }
    ],
    
    // array of slot objects
    "slots":
    [
        {
        "sid":"int:corresponds exactly to index in the tutor 'slots' array",
        "name":"string:name describing the slot (OFFICE=START-END)?",
        // 0=wrong office, 1=class in session, 2=other
        "courses":"array:ints in [0,2] describing preference level per course",
        }
    ]
}
